<div class="view view-sessions">
    <div class="view-content">
        <div class="item-list">
            <ul>

                {% for row in rows %}
                    {%
                    set row_classes = [
                    default_row_class ? 'views-row',
                    ]
                    %}

                    <li {{ row.attributes.addClass(row_classes) }}>
                        {{ row.content }}
                    </li>
                {% endfor %}

            </ul>
        </div>
    </div>
</div>
